<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fdc_neil.stylishgrabs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">




        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".USER.LoginActivity" />
        <activity android:name=".USER.CreateAccountActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".USER.ProfileActivity" />
        <activity android:name=".USER.EditProfileActivity" />
        <activity android:name=".AboutUsActivity"></activity>
    </application>

</manifest>